{
    "name": "coeliac/coeliac",
    "type": "project",
    "description": "Coeliac Sanctuary",
    "license": "proprietary",
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "algolia/algoliasearch-client-php": "^2.7",
        "algolia/scout-extended": "^1.10.1",
        "aws/aws-sdk-php": "^3.156",
        "bugsnag/bugsnag-laravel": "^2.0",
        "coeliac/address-lookup": "*",
        "coeliac/collection-items": "dev-develop",
        "coeliac/comments": "*",
        "coeliac/dispatch-slips": "*",
        "coeliac/image-manager": "*",
        "coeliac/order-dispatch-slip": "*",
        "coeliac/order-info": "*",
        "coeliac/order-shipped": "*",
        "coeliac/place-request-card": "*",
        "coeliac/shop-daily-stock": "*",
        "coeliac/shop-order-items": "*",
        "coeliac/shop-product-prices": "*",
        "coeliac/shop-product-sold-count": "*",
        "coeliac/shop-product-variants": "*",
        "coeliac/tag-manager": "*",
        "coeliac/wte-attractions": "dev-develop",
        "coeliac/wte-ratings-card": "*",
        "doctrine/dbal": "^2.11.1",
        "dompdf/dompdf": "^0.8.6",
        "fideloper/proxy": "^4.4.0",
        "fruitcake/laravel-cors": "^2.0.2",
        "guzzlehttp/guzzle": "^7.0.1",
        "intervention/image": "^2.5.1",
        "jpeters/architect": "^0.5.7",
        "jpeters/laravel-page-view": "^0.1.5",
        "laravel/framework": "^8.0",
        "laravel/horizon": "^5.2",
        "laravel/scout": "^8.3.1",
        "laravel/tinker": "^2.4.2",
        "league/flysystem-aws-s3-v3": "^1.0",
        "mattiasgeniar/php-percentages": "^1.1",
        "paypal/rest-api-sdk-php": "^1.14",
        "predis/predis": "^1.1",
        "spatie/geocoder": "^3.9.2",
        "spatie/laravel-backup": "^6.13",
        "spatie/laravel-cookie-consent": "^2.12.9",
        "spatie/laravel-mailcoach": "^3.0.0",
        "spatie/laravel-mailcoach-ses-feedback": "^2.0",
        "spatie/laravel-mix-preload": "^1.2",
        "stripe/stripe-php": "^7.57"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "^3.5.1",
        "beyondcode/helo-laravel": "^1.1.0",
        "beyondcode/laravel-dump-server": "^1.5",
        "facade/ignition": "^2.3.6",
        "friendsofphp/php-cs-fixer": "^2.16",
        "fzaninotto/faker": "^1.9",
        "jpeters/architect-test-helpers": "^0.2.2",
        "laravel/legacy-factories": "^1.0.0",
        "mockery/mockery": "^1.4.2",
        "nunomaduro/collision": "^5.0",
        "nunomaduro/larastan": "^0.6.4",
        "nunomaduro/phpinsights": "^1.14",
        "phploc/phploc": "^6.0",
        "phpmd/phpmd": "^2.9.1",
        "phpunit/phpunit": "^9.0",
        "spatie/test-time": "^1.2",
        "squizlabs/php_codesniffer": "^3.5",
        "wnx/laravel-stats": "^2.4.1"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "secure-http": false
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        },
        "hooks": {
            "pre-push": [
                "vendor/bin/phpunit"
            ]
        }
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/jpeters8889/architect"
        },
        {
            "type": "vcs",
            "url": "https://github.com/jpeters8889/architect-test-helpers"
        },
        {
            "type": "path",
            "url": "./architect/plans/tag-manager"
        },
        {
            "type": "path",
            "url": "./architect/plans/image-manager"
        },
        {
            "type": "path",
            "url": "./architect/plans/address-lookup"
        },
        {
            "type": "path",
            "url": "./architect/cards/comments"
        },
        {
            "type": "path",
            "url": "./architect/cards/wte-ratings-card"
        },
        {
            "type": "path",
            "url": "./architect/cards/place-request-card"
        },
        {
            "type": "path",
            "url": "./architect/plans/shop-product-variants"
        },
        {
            "type": "path",
            "url": "./architect/plans/shop-product-prices"
        },
        {
            "type": "path",
            "url": "./architect/plans/shop-product-sold-count"
        },
        {
            "type": "path",
            "url": "./architect/plans/coeliac-shop-order-items"
        },
        {
            "type": "path",
            "url": "./architect/plans/order-shipped"
        },
        {
            "type": "path",
            "url": "./architect/plans/order-info"
        },
        {
            "type": "path",
            "url": "./architect/plans/order-dispatch-slip"
        },
        {
            "type": "path",
            "url": "./architect/pages/dispatch-slips"
        },
        {
            "type": "path",
            "url": "./architect/cards/shop-daily-stock"
        },
        {
            "type": "path",
            "url": "./architect/plans/wte-attractions"
        },
        {
            "type": "path",
            "url": "./architect/plans/wte-attractions"
        },
        {
            "type": "path",
            "url": "./architect/plans/wte-attractions"
        },
        {
            "type": "path",
            "url": "./architect/plans/wte-attractions"
        },
        {
            "type": "path",
            "url": "./architect/plans/wte-attractions"
        },
        {
            "type": "composer",
            "url": "https://satis.mailcoach.app"
        },
        {
            "type": "path",
            "url": "./architect/plans/collection-items"
        }
    ],
    "autoload": {
        "psr-4": {
            "Coeliac\\": "App/",
            "Imports\\": "imports/"
        },
        "classmap": [
            "database/seeds",
            "database/factories",
            "imports"
        ],
        "files": [
            "App/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag mailcoach-assets --force"
        ],
        "cghooks": "vendor/bin/cghooks"
    }
}

        ],
        "files": [
            "App/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/",
            "Database\\Factories\\": "database/factories"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "scripts": {
        "fixer": "vendor/bin/php-cs-fixer fix --ansi --config=php-cs-fixer.dist.php -vvv",
        "phpstan": "vendor/bin/phpstan analyse",

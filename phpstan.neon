includes:
    - ./vendor/nunomaduro/larastan/extension.neon

parameters:

    paths:
        - App

    # The level 8 is the highest level
    level: 8

    ignoreErrors:
        - '#Anonymous function has an unused use \$router.#'

    excludePaths:
        - ./*/*/*Blueprint.php
        - ./*/*/*Dashboard.php
        - ./*/*/*Feed.php

    checkMissingIterableValueType: false
    noUnnecessaryCollectionCall: false
